<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:drawable="0x10800b3" android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="3dp" />
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="@color/installation_process_highlight_color_pressed" />
                    <stroke android:width="1dp" android:color="@color/installation_process_highlight_color_pressed" />
                    <corners android:radius="2dp" />
                </shape>
            </item>
            <item android:state_enabled="true">
                <shape>
                    <solid android:color="@color/installation_process_highlight_color" />
                    <stroke android:width="1dp" android:color="@color/installation_process_highlight_color" />
                    <corners android:radius="2dp" />
                </shape>
            </item>
            <item android:state_enabled="false">
                <shape>
                    <solid android:color="@color/cooling_installation_process_disabled" />
                    <stroke android:width="1dp" android:color="@color/cooling_installation_process_disabled" />
                    <corners android:radius="2dp" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
